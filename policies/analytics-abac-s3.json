{
    "Version": "2012-10-17",
    "Statement": [
      {
        "Sid": "ABACAnalyticsS3",
        "Effect": "Allow",
        "Action": [
          "s3:ListBucket",
          "s3:GetObject",
          "s3:PutObject"
        ],
        "Resource": [
          "arn:aws:s3:::*",
          "arn:aws:s3:::*/*"
        ],
        "Condition": {
          "StringEquals": {
            "aws:ResourceTag/team": "${aws:PrincipalTag/team}"
          }
        }
      }
    ]
  }
  